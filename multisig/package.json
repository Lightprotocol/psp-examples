{
  "name": "multisig-cli",
  "version": "0.0.1",
  "description": "PSP multisig",
  "author": "Sergey Timoshin @sergeytimoshin",
  "bin": {
    "multisig": "./bin/run"
  },
  "homepage": "https://github.com/lightprotocol/psp-examples/tree/main/multisig",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "lightprotocol/psp-examples",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],

  "scripts": {
    "lint:fix": "yarn prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "yarn prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "kill": "killall solana-test-validator || true && killall solana-test-val || true && sleep 1",
    "test-psp": "light psp:test multisig Fg6PaFpoGXkYsidMpWTK6W2BeZ7FEfcYkg476zPFsLnS",
    "test-cli": "yarn test-cli-create && yarn test-cli-approve && yarn test-cli-execute",
    "test-cli-create": "mocha ./tests/commands/create/index.test.ts --exit",
    "test-cli-approve": "mocha ./tests/commands/approve/index.test.ts --exit",
    "test-cli-execute": "mocha ./tests/commands/execute/index.test.ts --exit",
    "test-cli-all": "mocha --forbid-only \"tests/**/*.test.ts\"",
    "test": "yarn kill && yarn test-circuit && yarn test-cli-all",
    "build-circuit": "light psp:build multisig --ptau=17",
    "build": "shx rm -rf dist && yarn tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.5.0",
    "@coral-xyz/anchor": "^0.28.0",
    "@lightprotocol/cli": "file:../../light-protocol/cli",    
    "@lightprotocol/zk.js": "file:../../light-protocol/light-zk.js",
    "@project-serum/anchor": "^0.26.0",
    "@sqds/sdk": "^2.0.4",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7"
  },
  "devDependencies": {    
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@oclif/test": "^2.5.3",
    "oclif": "^3.15.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "oclif": {
    "bin": "multisig",
    "dirname": "multisig",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "bugs": "https://github.com/lightprotocol/multisig-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
